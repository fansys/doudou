<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doudou.dao.mapper.UserMessageMapper">

	<resultMap id="BaseResultMap" type="com.doudou.dao.entity.UserMessage">
		<id column="id" property="id" />
		<result column="tx_id" property="txId" />
		<result column="client_id" property="clientId" />
		<result column="type" property="type" />
		<result column="title" property="title" />
		<result column="content" property="content" />
		<result column="remark" property="remark" />
		<result column="created" property="created" />
		<result column="modified" property="modified" />
		<result column="flag" property="flag" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, tx_id AS txId, client_id AS clientId, type, title, content, remark, created, modified, flag
    </sql>
</mapper>
