<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doudou.dao.mapper.AdsMapper">

	<resultMap id="BaseResultMap" type="com.doudou.dao.entity.Ad">
		<id column="id" property="id" />
		<result column="ad_id" property="adId" />
		<result column="title" property="title" />
		<result column="description" property="description" />
		<result column="url" property="url" />
		<result column="image_url" property="imageUrl" />
		<result column="status" property="status" />
		<result column="remark" property="remark" />
		<result column="created" property="created" />
		<result column="modified" property="modified" />
		<result column="flag" property="flag" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, ad_id AS adId, title, description, url, image_url AS imageUrl, status, remark, created, modified, flag
    </sql>
</mapper>
